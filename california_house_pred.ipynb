      "source": [
        "# Creat pipeline for preprocessing nominal values\n",
        "num_attribs= list(housing_num)\n",
        "\n",
        "cat_attribs = [\"ocean_proximity\"]\n",
        "\n",
        "full_pipeline= ColumnTransformer([(\"num\", num_pipeline ,num_attribs ),\n",
